{
  "author": "João Guilherme <joao@guicheffer.me> (http://guicheffer.me/)",
  "name": "cheffplate",
  "version": "0.1.1",
  "description": "Front End™ compiler boilerplate with Webpack + React+Redux + Stylus  (w/ flexbox-grid) + Karma and Custom architecture to build custom apps from zero.",
  "homepage": "https://github.com/guicheffer/cheffplate#readme",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react",
    "redux",
    "karma",
    "stylus"
  ],
  "config": {
    "base": "./scripts/base.js",
    "style": "./styles/base.styl",
    "host": "localhost",
    "protocol": "http://",
    "port": 3000,
    "path": {
      "src": "src/",
      "build": "build/"
    },
    "env": {
      "dev": "development",
      "test": "test",
      "prod": "production"
    }
  },
  "scripts": {
    "env": "node -p \"require('./package.json').config;\"",
    "clean": "rimraf build && mkdir build",
    "prestart": "npm stop",
    "prebuild": "npm stop",
    "prepublish": "npm stop",
    "pretest": "rimraf reports",
    "stop": "npm run clean",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=$npm_package_config_env_dev ./node_modules/.bin/webpack -d --hot --inline --watch --progress",
    "start:build": "NODE_ENV=$npm_package_config_env_prod ./node_modules/.bin/webpack -p --progress",
    "test": "NODE_ENV=$npm_package_config_env_test ./node_modules/.bin/karma test",
    "test:ci": "watch 'npm test' $npm_package_config_path_src"
  },
  "main": "$npm_package_config_main",
  "engines": {
    "node": ">=5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guicheffer/cheffplate.git"
  },
  "bugs": {
    "email": "hi@guicheffer.me",
    "url": "https://github.com/guicheffer/cheffplate/issues"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "lwip": "git://github.com/chadwatson/lwip.git#fix-node6",
    "path": "^0.12.7",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "react-redux": "^4.4.5",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "sockjs-client": "^1.1.1",
    "sprite-webpack-plugin": "^0.3.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
